{
    "name": "Censor Content",
    "version": "1.0",
    "description": "Censor unwanted content on this page",
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["cc-app.js"],
        "style": ["style.css"]
    }],
    "permissions": ["storage"],
    "options_ui": {
        "page": "opt.html",
        "open_in_tab": true

    },
    "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "Click here to open menu",
        "default_popup": "opt.html"
    },
    "icons": {
        "96": "images/ico96png"
    }
}